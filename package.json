{
  "name": "@johnhoro/visualization-library",
  "version": "1.0.1",
  "description": "A modern Sankey diagram visualization library for creating beautiful flow charts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "sankey-diagram",
    "flow-chart",
    "data-flow",
    "visualization",
    "d3",
    "react",
    "typescript",
    "data-visualization"
  ],
  "author": "John Horo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnNesterLabs/visualization-library.git"
  },
  "bugs": {
    "url": "https://github.com/JohnNesterLabs/visualization-library/issues"
  },
  "homepage": "https://github.com/JohnNesterLabs/visualization-library#readme",
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.0.5",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "d3": "^7.8.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}